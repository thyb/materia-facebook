{
  "name": "@materia/facebook",
  "version": "1.1.2",
  "description": "Integrate Facebook connect to your Materia applications",
  "author": "team@getmateria.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/materiahq/materia-facebook/issues"
  },
  "homepage": "https://github.com/materiahq/materia-facebook#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/materiahq/materia-facebook.git"
  },
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "build:prod": "npm run build:client && npm run build:server && ncp LICENSE ./dist/LICENSE && ncp README.md ./dist/README.md && npm run postbuild",
    "build:server": "tsc -p ./tsconfig.server.json && ncp ./server/models ./dist/server/models && ncp ./server/api.json ./dist/server/api.json && ncp ./server/controllers ./dist/server/controllers",
    "build:client": "ng build --prod facebook",
    "postbuild": "node scripts/postbuild.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "fbgraph": "^1.4.4",
    "passport-facebook": "^2.1.1",
    "passport-facebook-token": "^3.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular-devkit/build-ng-packagr": "~0.901.4",
    "@angular/animations": "^9.1.4",
    "@angular/cdk": "^9.2.2",
    "@angular/cli": "~9.1.4",
    "@angular/common": "^9.1.4",
